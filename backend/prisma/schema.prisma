generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  name     String
  email    String? @unique
  password String?
  image    String?
  githubId String? @unique
  googleId String? @unique
}

model Problems {
  id          Int    @id @default(autoincrement())
  title       String
  description String
  difficulty  String
  testCases   String
}
